    %issuetitle         = {issuetitle},
    %issuesubtitle      = {issuesubtitle},
    %language           = {danish}, %UKenglish, USenglish
    %origlanguage       = {danish},
    %series             = {series},
    %volume             = {volume},
    %number             = {number},
    %eid                = {eid}, %electronic identifier
    %issue              = {issue},
    %month              = {month},   % Use with year
    %pages              = {pages},
    %version            = {version},
    %note               = {note},
    %issn               = {issn},    % International Standard Technical Report Number
    %addendum           = {addendum},
    %pubstate           = {pubstate},    %inpreparation, submitted, forthcoming, inpress, prepublished
    %doi                = {doi},     % Digital Object Identifer
    %eprint             = {eprint},  % The electronic identifier of an online publication
    %eprintclass        = {eprintclass},
    %eprinttype         = {eprinttype},
    %url                = {url},
    %urldate            = {urldate}, % Last seen
}

